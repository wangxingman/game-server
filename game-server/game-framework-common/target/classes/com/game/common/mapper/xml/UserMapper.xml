<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.game.common.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.game.common.entity.user.User">
        <id column="v_id" property="vId" />
        <result column="v_name" property="vName" />
        <result column="v_number" property="vNumber" />
        <result column="v_email" property="vEmail" />
        <result column="v_phone" property="vPhone" />
        <result column="v_token" property="vToken" />
        <result column="v_account" property="vAccount" />
        <result column="v_pass" property="vPass" />
        <result column="v_createtime" property="vCreatetime" />
        <result column="v_updatetime" property="vUpdatetime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        v_id AS vId, v_name AS vName, v_number AS vNumber, v_email AS vEmail, v_phone AS vPhone, v_token AS vToken, v_account AS vAccount, v_pass AS vPass, v_createtime AS vCreatetime, v_updatetime AS vUpdatetime
    </sql>

</mapper>
